<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="description" content="">
    <meta name="author" content="">

    <title>MTB - Admin Page</title>

    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="{{ asset('css/bootstrap.css') }}">

    <!-- Custom styles for this template -->
    <link href="{{ asset("css/jumbotron-narrow.css") }}" rel="stylesheet">
    <link rel="stylesheet" href="{{ asset('css/fonts.css') }}">

    <!-- Favicon -->
    <link rel="icon" type="image/x-icon" href="{{ asset('img/favicon.jpg') }}" />

    <!-- Google Fonts -->
    <link href='https://fonts.googleapis.com/css?family=Dosis:400,500,600,700' rel='stylesheet' type='text/css' />
    <link href="https://fonts.googleapis.com/css?family=Cantarell|Germania+One" rel="stylesheet">

    <!-- DataTable CSS Bootstrap -->
    <link href="https://cdn.datatables.net/1.10.16/css/dataTables.bootstrap.min.css">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body>

<div class="container">
    <div class="header clearfix">
        <nav>
            <ul class="nav nav-pills pull-right">
                <li {% if app.request.get('_route') is same as('liveroom.index') %}class="active" {% endif %}role="presentation"><a href="{{ path('liveroom.index') }}">Salles de concert</a></li>
                <li {% if app.request.get('_route') is same as('store.index') %}class="active" {% endif %}role="presentation"><a href="{{ path('store.index') }}">Magasins</a></li>
                <li {% if app.request.get('_route') is same as('offer.index') %}class="active" {% endif %}role="presentation"><a href="{{ path('offer.index') }}">Petites Annonces</a></li>
                <li {% if app.request.get('_route') is same as('concert.index') %}class="active" {% endif %}role="presentation"><a href="{{ path('concert.index') }}">Concerts</a></li>
                <li {% if app.request.get('_route') is same as('instrument.index') %}class="active" {% endif %}role="presentation"><a href="{{ path('instrument.index') }}">Instruments</a></li>
                <li {% if app.request.get('_route') is same as('musicCategory.index') %}class="active" {% endif %}role="presentation"><a href="{{ path('musicCategory.index') }}">Style</a></li>
                <li {% if app.request.get('_route') is same as('musicianCategory.index') %}class="active" {% endif %}role="presentation"><a href="{{ path('musicianCategory.index') }}">Musiciens</a></li>
            </ul>
        </nav>
        <img class="logo" src="{{ asset('img/Logo_2-white.svg') }}"> <h3 class="text-muted">MTB ADMIN</h3>
    </div>

    <div class="row marketing">
        <div class="col-lg-12">
            {% block content %}

            {% endblock %}
        </div>
    </div>

    <footer class="footer">
        <p>&copy; 2017 Steven Heven, Inc.</p>
    </footer>

</div> <!-- /container -->




<!-- jQuery -->
<script src="{{ asset("js/jquery.min.js") }}"></script>
<!-- Bootstrap Core JavaScript -->
<script src="{{ asset("js/bootstrap.min.js") }}"></script>
<!-- jQuery DataTables -->
<script type="text/javascript" src="//cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js"></script>
<!-- Bootstrap JS DataTables -->
<script type="text/javascript" src="//cdn.datatables.net/1.10.16/js/dataTables.bootstrap.min.js"></script>
<!-- Site Scripts -->
<script src="{{ asset("js/main.js") }}"></script>
</body>
</html>

